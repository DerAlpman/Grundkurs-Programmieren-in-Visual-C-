        <Label Content="ggT:" Height="28" HorizontalAlignment="Left" Margin="20,50,0,0" Name="label1" VerticalAlignment="Top" />
        <Label Content="kgV:" Height="28" HorizontalAlignment="Left" Margin="20,80,0,0" Name="label2" VerticalAlignment="Top" />

<Window x:Class="Aufgabe_3._5.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Division ohne Rest" Height="180" Width="170">
    <Grid>
        <TextBox Height="23" HorizontalAlignment="Left" Margin="20,20,0,0" Name="txtA" VerticalAlignment="Top" Width="50" HorizontalContentAlignment="Right" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="80,20,0,0" Name="txtB" VerticalAlignment="Top" Width="50" HorizontalContentAlignment="Right" />
        <TextBox Height="23" HorizontalAlignment="Left" HorizontalContentAlignment="Center" Margin="20,100,0,0" Name="txtResult" VerticalAlignment="Top" Width="110" IsEnabled="False" IsReadOnly="False" />
        <Button Content="Berechnen" Height="23" HorizontalAlignment="Center" Margin="0,60,0,0" Name="btnCalculate" VerticalAlignment="Top" Width="75" Click="btnCalculate_Click" />
    </Grid>
</Window>

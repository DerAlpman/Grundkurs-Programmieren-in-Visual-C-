<Window x:Class="Aufgabe_2._4.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Standardisierte Normalverteilung" Height="160" Width="200">
    <Grid>
        <Label Content="u:" Height="28" HorizontalAlignment="Left" Margin="20,20,0,0" Name="label1" VerticalAlignment="Top" />
        <Label Content="phi(u):" Height="28" HorizontalAlignment="Left" Margin="20,80,0,0" Name="label2" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="50,20,0,0" Name="txtU" VerticalAlignment="Top" Width="50" HorizontalContentAlignment="Right" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="80,80,0,0" Name="txtResult" VerticalAlignment="Top" Width="50" IsEnabled="False" HorizontalContentAlignment="Right" />
        <Button Content="Berechnen" Height="23" HorizontalAlignment="Left" Margin="40,50,0,0" Name="btnCalculate" VerticalAlignment="Top" Width="75" Click="btnCalculate_Click" />
    </Grid>
</Window>
